T = int(input())

answer = []


for j in range(1,T+1):
    
    cnt = str(j)   #문자열로 접근
    
    line = ""
    
    for k in cnt:
        if int(k)%3 == 0 and int(k) != 0:
            line += "-"
        
        else:
            continue
    
    if line:
        answer.append(line)
    else:
        answer.append(cnt)

print(" ".join(answer))
            
            
    
            
            



