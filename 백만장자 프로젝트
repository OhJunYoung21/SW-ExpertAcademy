def func(prices):
    
    count = list(reversed(prices))
    max_value = count[0]
    
    profit = 0
    
    for k in count[1:]:
        if k >= max_value:
            max_value = k 
        
        else:
            profit += (max_value - k)
        
        
    return profit


print(func([1,1,3,1,2]))
                     
            
            
    
